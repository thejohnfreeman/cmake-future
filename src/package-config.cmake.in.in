@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
foreach(PACKAGE_NAME @PROJECT_DEPENDENCIES@)
  # TODO: Pass along the rest of the options that were passed to
  # `future_add_dependency`.
  find_dependency("${PACKAGE_NAME}" REQUIRED)
endforeach(PACKAGE_NAME @PROJECT_DEPENDENCIES@)

if(NOT TARGET @PROJECT_NAME@::@PROJECT_NAME@)
  include(${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-targets.cmake)
endif()
